{
    "name": "capstone-server",
    "version": "1.0.49",
    "description": "My Capstone",
    "engines": {
        "node": "14.17.5"
    },
    "scripts": {
        "prebuild": "rm -rf ./node_modules && npm i",
        "build": "rm -rf ./build && tsc -p ./src && mv ./build/src/* ./build/ && rmdir ./build/src",
        "format": "prettier --write \"**/*.ts\"",
        "format-check": "prettier --check \"**/*.ts\"",
        "dev": "nodemon -e ts,.env,json --ignore build/ --ignore test/ --exec ./node_modules/.bin/ts-node ./src/index.ts",
        "start": "node ./index.js",
        "pretest:cucumber": "cd ./test",
        "test:cucumber": "../node_modules/.bin/cucumber-js -p default -f @cucumber/pretty-formatter --exit",
        "posttest:cucumber": "cd ../",
        "pretest:jest": "cd ./test",
        "test:jest": "cd ./test && jest --maxConcurrency=1 --runInBand --coverage",
        "posttest:jest": "cd ../",
        "test": "npm run test:jest && npm run test:cucumber"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/noahvarghese/capstone-server.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/noahvarghese/capstone-server/issues"
    },
    "homepage": "https://github.com/noahvarghese/capstone-server#readme",
    "devDependencies": {
        "@babel/preset-typescript": "^7.13.0",
        "@cucumber/cucumber": "^7.0.0",
        "@cucumber/pretty-formatter": "^1.0.0-alpha.0",
        "@types/axios": "^0.14.0",
        "@types/bcryptjs": "^2.4.2",
        "@types/chai": "^4.2.17",
        "@types/cookie-parser": "^1.4.2",
        "@types/cors": "^2.8.10",
        "@types/csurf": "^1.11.1",
        "@types/cucumber": "^7.0.0",
        "@types/express": "^4.17.11",
        "@types/express-fileupload": "^1.1.6",
        "@types/express-mysql-session": "^2.1.2",
        "@types/express-session": "^1.17.3",
        "@types/google-libphonenumber": "^7.4.21",
        "@types/jest": "^26.0.23",
        "@types/mysql": "^2.15.18",
        "@types/node": "^14.14.41",
        "@types/node-fetch": "^2.5.10",
        "@types/nodemailer": "^6.4.1",
        "@types/passport": "^1.0.6",
        "@types/passport-saml": "^1.1.2",
        "@types/validator": "^13.1.3",
        "@typescript-eslint/eslint-plugin": "^4.22.0",
        "@typescript-eslint/parser": "^4.22.0",
        "axios": "^0.21.4",
        "chai": "^4.3.4",
        "dotenv": "^8.2.0",
        "eslint": "^7.24.0",
        "formdata": "^0.10.4",
        "jest": "^26.6.3",
        "nodemon": "^2.0.7",
        "prettier": "^2.4.0",
        "ts-jest": "^26.5.5",
        "ts-node": "^9.1.1",
        "typescript": "^4.2.4"
    },
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "cluster": "^0.7.7",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "csurf": "^1.11.0",
        "express": "^4.17.1",
        "express-fileupload": "^1.2.1",
        "express-mysql-session": "^2.1.5",
        "express-session": "^1.17.1",
        "fs": "^0.0.1-security",
        "google-libphonenumber": "^3.2.22",
        "memorystore": "^1.6.6",
        "module-alias": "^2.2.2",
        "mysql": "^2.18.1",
        "node-fetch": "^2.6.1",
        "nodemailer": "^6.5.0",
        "passport": "^0.4.1",
        "passport-saml": "^2.1.0",
        "path": "^0.12.7",
        "rand-token": "^1.0.1",
        "ts-transformer-keys": "^0.4.3",
        "typeorm": "^0.2.32",
        "validator": "^13.6.0"
    },
    "_moduleAliases": {
        "@models": "models",
        "@routes": "routes",
        "@util": "util"
    }
}
